package mpkpk.project.hotel;

public class NormalRoom extends RoomDecorator {

	private int id;
	private boolean isVip = false;
	
	public int GetId() {
		return id;
	}
	public boolean CheckIfVip()
	{
		if (isVip)
		{
			return true;
		}
		else
		{
			return false;
		}
	}
	public VipRoom(Room decoratedRoom)
	{
		super(decoratedRoom);
	}
}
